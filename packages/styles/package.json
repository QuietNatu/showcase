{
  "name": "@natu/styles",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./assets/*": "./src/assets/*",
    "./stylesheets/*": "./src/stylesheets/*",
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "tsx ./src/build-tokens.ts",
    "lint": "tsc --noEmit && eslint . --report-unused-disable-directives --no-warn-ignored --max-warnings 0",
    "storybook": "storybook dev -p 6013",
    "storybook:build": "storybook build",
    "storybook:preview": "vite preview --port 6013 --outDir ./storybook-static"
  },
  "dependencies": {
    "modern-normalize": "3.0.1"
  },
  "devDependencies": {
    "@natu/axe": "workspace:^",
    "@natu/eslint-config-shared": "workspace:^",
    "@natu/stories": "workspace:^",
    "@natu/tsconfig": "workspace:^",
    "@storybook/addon-a11y": "9.1.2",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/react-vite": "9.1.2",
    "@types/node": "22.17.2",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react-swc": "4.0.1",
    "eslint": "9.33.0",
    "picocolors": "1.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "storybook": "9.1.2",
    "style-dictionary": "5.0.2",
    "tsx": "4.20.4",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "7.1.3"
  }
}
