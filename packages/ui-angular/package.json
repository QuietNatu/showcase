{
  "name": "@natu/ui-angular",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./test": {
      "types": "./src/lib/test/index.ts",
      "default": "./src/lib/test/index.ts"
    },
    "./stories": {
      "types": "./src/lib/stories/index.ts",
      "default": "./src/lib/stories/index.ts"
    }
  },
  "scripts": {
    "lint": "eslint --report-unused-disable-directives --no-warn-ignored --max-warnings 0",
    "test": "ng test --code-coverage",
    "test-watch": "ng test --watch --browsers Chrome",
    "storybook": "ng run lib:storybook",
    "build-storybook": "ng run lib:build-storybook",
    "preview-storybook": "http-server -c-1 ./storybook-static --silent",
    "vrt": "playwright test --config vrt/playwright.config.ts",
    "vrt-update": "pnpm vrt --update-snapshots",
    "vrt-show-report": "playwright show-report vrt/report --port 6114",
    "eslint-inspector": "pnpm dlx @eslint/config-inspector@latest"
  },
  "peerDependencies": {
    "@angular/animations": "19.0.1",
    "@angular/cdk": "19.0.1",
    "@angular/common": "19.0.1",
    "@angular/core": "19.0.1",
    "@angular/forms": "19.0.1",
    "@angular/router": "19.0.1",
    "@floating-ui/dom": "1.6.12",
    "@natu/assets": "workspace:*",
    "@natu/styles": "workspace:*",
    "@natu/utils": "workspace:*",
    "class-variance-authority": "0.7.1",
    "date-fns": "4.1.0",
    "i18next": "24.0.2",
    "rxjs": "7.8.1",
    "type-fest": "4.26.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.4",
    "@angular/animations": "19.0.1",
    "@angular/cdk": "19.0.1",
    "@angular/cli": "19.0.4",
