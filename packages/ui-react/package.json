{
  "name": "@natu/ui-react",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "default": "./src/lib/index.ts"
    },
    "./components/*": {
      "default": "./src/lib/components/*/index.ts"
    }
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint . --report-unused-disable-directives --no-warn-ignored --max-warnings 0",
    "storybook": "storybook dev -p 6013",
    "storybook:build": "storybook build",
    "storybook:preview": "vite preview --port 6013 --outDir ./storybook-static",
    "test": "vitest run --coverage",
    "test:watch": "cross-env TEST_DEBUG=1 vitest"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@natu/axe": "workspace:^",
    "@natu/eslint-config-shared": "workspace:^",
    "@natu/stories": "workspace:^",
    "@natu/styles": "workspace:^",
    "@natu/tsconfig": "workspace:^",
    "@storybook/addon-a11y": "9.1.2",
    "@storybook/addon-docs": "9.1.2",
    "@storybook/react-vite": "9.1.2",
    "@types/node": "22.17.2",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react-swc": "4.0.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "cross-env": "10.0.0",
    "eslint": "9.33.0",
    "globals": "16.3.0",
    "playwright": "1.54.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "storybook": "9.1.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.39.1",
    "vite": "7.1.2",
    "vitest": "3.2.4",
    "vitest-browser-react": "1.0.1"
  }
}
