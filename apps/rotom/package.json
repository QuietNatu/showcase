{
  "name": "@natu/rotom",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build",
    "preview": "http-server -c-1 dist/browser --silent",
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "test-watch": "ng test --watch --browsers Chrome",
    "analyse": "pnpm build --source-map true --named-chunks true && source-map-explorer dist/**/*.js",
    "storybook": "ng run rotom:storybook",
    "build-storybook": "ng run rotom:build-storybook",
    "preview-storybook": "http-server -c-1 ./storybook-static --silent",
    "vrt": "playwright test --config vrt/playwright.config.ts",
    "vrt-update": "pnpm vrt --update-snapshots",
    "vrt-show-report": "playwright show-report vrt/report --port 6103",
    "e2e": "playwright test --config e2e/playwright.config.ts"
  },
  "dependencies": {
    "@angular/animations": "17.0.6",
    "@angular/common": "17.0.6",
    "@angular/compiler": "17.0.6",
    "@angular/core": "17.0.6",
    "@angular/forms": "17.0.6",
    "@angular/platform-browser": "17.0.6",
    "@angular/platform-browser-dynamic": "17.0.6",
    "@angular/router": "17.0.6",
    "@angular/service-worker": "17.0.6",
    "@natu/styles": "workspace:*",
    "@natu/ui-angular": "workspace:*",
    "@natu/utils": "workspace:*",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.6",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular/cli": "17.0.6",
    "@angular/compiler-cli": "17.0.6",
    "@compodoc/compodoc": "1.1.23",
    "@mswjs/data": "0.16.1",
    "@natu/eslint-config-angular": "workspace:*",
    "@natu/tsconfig": "workspace:*",
    "@natu/vrt": "workspace:*",
    "@playwright/test": "1.40.1",
    "@storybook/addon-a11y": "7.6.3",
    "@storybook/addon-docs": "7.6.3",
    "@storybook/addon-essentials": "7.6.3",
    "@storybook/addon-interactions": "7.6.3",
    "@storybook/addon-links": "7.6.3",
    "@storybook/angular": "7.6.3",
    "@storybook/blocks": "7.6.3",
    "@storybook/test": "7.6.3",
    "@testing-library/angular": "15.1.0",
    "@testing-library/jasmine-dom": "1.3.3",
    "@testing-library/user-event": "14.5.1",
    "@types/jasmine": "5.1.4",
    "@types/node": "20.10.4",
    "@types/testing-library__jasmine-dom": "1.3.3",
    "cross-env": "7.0.3",
    "eslint": "8.55.0",
    "http-server": "14.1.1",
    "jasmine-axe": "1.1.0",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "msw": "2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "source-map-explorer": "2.5.3",
    "storybook": "7.6.3",
    "typescript": "5.2.2"
  },
  "msw": {
    "workerDirectory": "src"
  }
}
