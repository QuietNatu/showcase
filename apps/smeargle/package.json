{
  "name": "@natu/smeargle",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --report-unused-disable-directives --no-warn-ignored --max-warnings 0",
    "test": "vitest run --coverage",
    "test-watch": "vitest",
    "analyse": "pnpm build --sourcemap && source-map-explorer dist/assets/**/*.js --html bundle-analysis.html",
    "storybook": "storybook dev -p 6002",
    "build-storybook": "storybook build",
    "preview-storybook": "http-server -c-1 ./storybook-static --silent",
    "vrt": "playwright test --config vrt/playwright.config.ts",
    "vrt-update": "pnpm vrt --update-snapshots",
    "vrt-show-report": "playwright show-report vrt/report --port 6004",
    "e2e": "cross-env VITE_E2E=true bddgen --config e2e/playwright.config.ts && playwright test --config e2e/playwright.config.ts",
    "build-dependencies": "pnpm build-api && pnpm build-i18n",
    "build-api": "orval --config ./orval.config.ts",
    "build-i18n": "bundle-i18n --source src/locales/original --destination src/locales/bundle --filename translation",
    "lighthouse-mobile": "lhci autorun",
    "lighthouse-desktop": "lhci autorun --collect.settings.preset=desktop --upload.outputDir=lighthouse-reports/desktop",
    "eslint-inspector": "pnpm dlx @eslint/config-inspector@latest"
  },
  "dependencies": {
    "@floating-ui/react": "0.26.28",
    "@natu/assets": "workspace:*",
    "@natu/styles": "workspace:*",
    "@natu/ui-react": "workspace:*",
    "@natu/utils": "workspace:*",
    "@radix-ui/react-collapsible": "1.1.1",
    "@radix-ui/react-slot": "1.1.0",
    "@react-aria/utils": "3.26.0",
    "axios": "1.7.7",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "i18next": "24.0.2",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-resources-to-backend": "1.2.1",
    "react": "18.3.1",
    "react-aria": "3.36.0",
    "react-dom": "18.3.1",
    "react-i18next": "15.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "9.2.0",
    "@lhci/cli": "0.14.0",
    "@mswjs/data": "0.16.2",
    "@natu/bundle-i18n": "workspace:*",
    "@natu/eslint-config": "workspace:*",
    "@natu/tsconfig": "workspace:*",
    "@natu/vite-plugin-bundle-i18n": "workspace:*",
    "@natu/vrt": "workspace:*",
    "@playwright/test": "1.49.0",
    "@storybook/addon-a11y": "8.4.5",
    "@storybook/addon-essentials": "8.4.5",
    "@storybook/addon-interactions": "8.4.5",
    "@storybook/addon-links": "8.4.5",
    "@storybook/addon-onboarding": "8.4.5",
    "@storybook/blocks": "8.4.5",
    "@storybook/react": "8.4.5",
    "@storybook/react-vite": "8.4.5",
    "@storybook/test": "8.4.5",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react-swc": "3.7.1",
    "@vitest/coverage-v8": "2.1.5",
    "autoprefixer": "10.4.20",
    "axe-core": "4.10.2",
    "browserslist-to-esbuild": "2.1.1",
    "cross-env": "7.0.3",
    "http-server": "14.1.1",
    "jsdom": "25.0.1",
    "msw": "2.4.9",
    "orval": "7.3.0",
    "playwright-bdd": "7.5.0",
    "prettier": "3.3.3",
    "source-map-explorer": "2.5.3",
    "storybook": "8.4.5",
    "type-fest": "4.26.1",
    "typescript": "5.5.4",
    "vite": "5.4.11",
    "vite-plugin-pwa": "0.21.0",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.3",
    "vitest": "2.1.5"
  }
}
