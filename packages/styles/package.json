{
  "name": "@natu/styles",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./assets/*": "./src/assets/*",
    "./stylesheets/*": "./src/stylesheets/*",
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "tsx ./src/build-tokens.ts",
    "lint": "tsc --noEmit && eslint . --report-unused-disable-directives --no-warn-ignored --max-warnings 0",
    "storybook": "storybook dev -p 6900",
    "storybook:build": "storybook build",
    "storybook:preview": "vite preview --port 6900 --outDir ./storybook-static",
    "test": "vitest run"
  },
  "dependencies": {
    "modern-normalize": "3.0.1"
  },
  "devDependencies": {
    "@natu/axe": "workspace:^",
    "@natu/eslint-config-shared": "workspace:^",
    "@natu/stories": "workspace:^",
    "@natu/tsconfig": "workspace:^",
    "@storybook/addon-a11y": "9.1.3",
    "@storybook/addon-docs": "9.1.3",
    "@storybook/react-vite": "9.1.3",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "22.19.1",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react-swc": "4.2.2",
    "colorjs.io": "0.5.2",
    "eslint": "9.33.0",
    "happy-dom": "20.0.10",
    "picocolors": "1.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "storybook": "9.1.3",
    "style-dictionary": "5.1.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.0",
    "vite": "7.2.4",
    "vitest": "3.2.4"
  }
}
