{
  "name": "@natu/styles",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "rimraf dist && pnpm build:tokens && pnpm build:stylesheets && pnpm build:assets",
    "build:assets": "copyfiles --up 1 ./src/assets/**/* ./dist",
    "build:stylesheets": "copyfiles --up 1 ./src/stylesheets/**/* ./dist",
    "build:tokens": "tsx ./src/build-tokens.ts",
    "storybook": "storybook dev -p 6013",
    "storybook:build": "storybook build",
    "storybook:preview": "vite preview --port 6013 --outDir ./storybook-static"
  },
  "devDependencies": {
    "@natu/axe": "workspace:^",
    "@natu/eslint-config-shared": "workspace:^",
    "@natu/tsconfig": "workspace:^",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@storybook/test": "^8.6.14",
    "@types/node": "22.14.1",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@vitejs/plugin-react-swc": "3.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "9.25.0",
    "picocolors": "^1.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rimraf": "^6.0.1",
    "storybook": "8.6.12",
    "style-dictionary": "^5.0.0",
    "tsx": "^4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "vite": "^6.3.1"
  }
}
