{
  "name": "@natu/ui-solid",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/lib/index.ts",
      "default": "./src/lib/index.ts"
    },
    "./test": {
      "types": "./src/lib/test/index.ts",
      "default": "./src/lib/test/index.ts"
    },
    "./test/extend-expect": {
      "types": "./src/lib/test/extend-expect.ts",
      "default": "./src/lib/test/extend-expect.ts"
    }
  },
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run --coverage",
    "test-watch": "vitest",
    "storybook": "storybook dev -p 6211",
    "build-storybook": "storybook build",
    "preview-storybook": "http-server -c-1 ./storybook-static --silent",
    "vrt": "playwright test --config vrt/playwright.config.ts",
    "vrt-update": "pnpm vrt --update-snapshots",
    "vrt-show-report": "playwright show-report vrt/report --port 6213"
  },
  "devDependencies": {
    "@natu/eslint-config-solid": "workspace:*",
    "@natu/styles": "workspace:^",
    "@natu/tsconfig": "workspace:*",
    "@natu/utils": "workspace:*",
    "@natu/vrt": "workspace:*",
    "@playwright/test": "1.40.0",
    "@solidjs/testing-library": "0.8.4",
    "@storybook/addon-a11y": "7.5.3",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/blocks": "7.5.3",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/user-event": "14.5.1",
    "@vitejs/plugin-react-swc": "3.4.1",
    "@vitest/coverage-v8": "0.34.6",
    "cross-env": "7.0.3",
    "http-server": "14.1.1",
    "jsdom": "22.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "solid-js": "1.8.6",
    "storybook": "7.5.3",
    "storybook-solidjs": "1.0.0-beta.2",
    "storybook-solidjs-vite": "1.0.0-beta.2",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-plugin-solid": "2.7.2",
    "vitest": "0.34.6",
    "vitest-axe": "0.1.0"
  }
}
