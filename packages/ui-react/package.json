{
  "name": "@natu/ui-react",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "default": "./src/lib/index.ts"
    },
    "./components/*": {
      "default": "./src/lib/components/*/index.ts"
    }
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint . --report-unused-disable-directives --no-warn-ignored --max-warnings 0",
    "storybook": "storybook dev -p 6013",
    "storybook:build": "storybook build",
    "storybook:preview": "vite preview --port 6013 --outDir ./storybook-static",
    "test": "vitest run --coverage",
    "test:watch": "cross-env TEST_DEBUG=1 vitest"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.25.0",
    "@natu/axe": "workspace:^",
    "@natu/eslint-config-shared": "workspace:^",
    "@natu/tsconfig": "workspace:^",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@storybook/test": "8.6.12",
    "@types/node": "22.14.1",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@vitejs/plugin-react-swc": "3.8.1",
    "@vitest/browser": "3.1.1",
    "@vitest/coverage-v8": "3.1.1",
    "cross-env": "7.0.3",
    "eslint": "9.25.0",
    "globals": "16.0.0",
    "playwright": "1.51.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "storybook": "8.6.12",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "vite": "^6.3.1",
    "vitest": "3.1.1",
    "vitest-browser-react": "0.1.1"
  }
}
