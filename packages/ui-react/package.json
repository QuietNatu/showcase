{
  "name": "@natu/ui-react",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/lib/index.ts",
      "default": "./src/lib/index.ts"
    },
    "./test": {
      "types": "./src/lib/test/index.ts",
      "default": "./src/lib/test/index.ts"
    },
    "./test/extend-expect": {
      "types": "./src/lib/test/extend-expect.ts",
      "default": "./src/lib/test/extend-expect.ts"
    }
  },
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run --coverage",
    "test-watch": "vitest",
    "storybook": "storybook dev -p 6011",
    "build-storybook": "storybook build",
    "preview-storybook": "http-server ./storybook-static --silent",
    "vrt": "playwright test --config vrt/playwright.config.ts",
    "vrt-update": "pnpm vrt --update-snapshots",
    "vrt-show-report": "playwright show-report vrt/report --port 6013",
    "vrt-debug": "cross-env PWDEBUG=1 pnpm vrt"
  },
  "devDependencies": {
    "@natu/eslint-config-react": "workspace:*",
    "@natu/tsconfig": "workspace:*",
    "@natu/utils": "workspace:*",
    "@natu/vrt": "workspace:*",
    "@playwright/test": "1.39.0",
    "@storybook/addon-a11y": "7.5.2",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.5.2",
    "@storybook/react": "7.5.2",
    "@storybook/react-vite": "7.5.2",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@vitejs/plugin-react-swc": "3.4.1",
    "@vitest/coverage-v8": "0.34.6",
    "cross-env": "^7.0.3",
    "http-server": "^14.1.1",
    "jsdom": "22.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.5.2",
    "typescript": "5.1.6",
    "vite": "4.5.0",
    "vitest": "0.34.6",
    "vitest-axe": "0.1.0"
  }
}
