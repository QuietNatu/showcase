{
  "name": "@natu/styles",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "types": "./ts/index.d.ts",
  "module": "./ts/index.js",
  "exports": {
    ".": {
      "types": "./ts/index.d.ts",
      "default": "./ts/index.js"
    },
    "./scss": {
      "default": "./scss/_index.scss"
    },
    "./scss/utils": {
      "default": "./scss/utils/_index.scss"
    }
  },
  "scripts": {
    "build": "pnpm clean && node ./tokens/config.js",
    "clean": "rimraf ./scss/tokens ./ts/tokens"
  },
  "dependencies": {
    "modern-normalize": "2.0.0"
  },
  "devDependencies": {
    "@natu/tsconfig": "workspace:*",
    "@types/node": "20.11.0",
    "rimraf": "5.0.5",
    "style-dictionary": "4.0.0-prerelease.7",
    "typescript": "5.3.3",
    "vite": "5.0.11"
  }
}
