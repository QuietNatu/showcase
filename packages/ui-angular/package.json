{
  "name": "@natu/ui-angular",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./test": {
      "types": "./src/lib/test/index.ts",
      "default": "./src/lib/test/index.ts"
    },
    "./stories": {
      "types": "./src/lib/stories/index.ts",
      "default": "./src/lib/stories/index.ts"
    }
  },
  "scripts": {
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "test-watch": "ng test --watch --browsers Chrome",
    "storybook": "ng run lib:storybook",
    "build-storybook": "ng run lib:build-storybook",
    "preview-storybook": "http-server -c-1 ./storybook-static --silent",
    "vrt": "playwright test --config vrt/playwright.config.ts",
    "vrt-update": "pnpm vrt --update-snapshots",
    "vrt-show-report": "playwright show-report vrt/report --port 6114"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.2",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular/animations": "18.2.2",
    "@angular/cdk": "18.2.2",
    "@angular/cli": "18.2.2",
    "@angular/common": "18.2.2",
    "@angular/compiler": "18.2.2",
    "@angular/compiler-cli": "18.2.2",
    "@angular/core": "18.2.2",
    "@angular/forms": "18.2.2",
    "@angular/platform-browser": "18.2.2",
    "@angular/platform-browser-dynamic": "18.2.2",
    "@angular/router": "18.2.2",
    "@compodoc/compodoc": "1.1.25",
    "@floating-ui/dom": "1.6.10",
    "@natu/assets": "workspace:*",
    "@natu/eslint-config-angular": "workspace:*",
    "@natu/styles": "workspace:*",
    "@natu/tsconfig": "workspace:*",
    "@natu/utils": "workspace:*",
    "@natu/vrt": "workspace:*",
    "@ngneat/svg-icon": "7.1.6",
    "@playwright/test": "1.46.1",
    "@storybook/addon-a11y": "8.2.9",
    "@storybook/addon-docs": "8.2.9",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/angular": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/test": "8.2.9",
    "@testing-library/angular": "16.0.0",
    "@testing-library/jasmine-dom": "1.3.3",
    "@testing-library/user-event": "14.5.2",
    "@types/jasmine": "5.1.4",
    "@types/node": "20.14.14",
    "@types/testing-library__jasmine-dom": "1.3.3",
    "axe-core": "4.9.1",
    "class-variance-authority": "0.7.0",
    "cross-env": "7.0.3",
    "date-fns": "3.6.0",
    "eslint": "8.57.0",
    "http-server": "14.1.1",
    "i18next": "23.14.0",
    "jasmine-axe": "1.1.0",
    "jasmine-core": "5.2.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-spec-reporter": "0.0.36",
    "ng-packagr": "18.2.1",
    "rxjs": "7.8.1",
