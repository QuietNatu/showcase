{
  "name": "@natu/rotom",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build",
    "preview": "http-server -c-1 dist --silent",
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "test-watch": "ng test --watch --browsers Chrome",
    "analyse": "pnpm build --source-map true --named-chunks true && source-map-explorer dist/**/*.js",
    "storybook": "ng run rotom:storybook",
    "build-storybook": "ng run rotom:build-storybook",
    "preview-storybook": "http-server -c-1 ./storybook-static --silent",
    "vrt": "playwright test --config vrt/playwright.config.ts",
    "vrt-update": "pnpm vrt --update-snapshots",
    "vrt-show-report": "playwright show-report vrt/report --port 6103",
    "e2e": "playwright test --config e2e/playwright.config.ts"
  },
  "dependencies": {
    "@angular/animations": "16.2.12",
    "@angular/common": "16.2.12",
    "@angular/compiler": "16.2.12",
    "@angular/core": "16.2.12",
    "@angular/forms": "16.2.12",
    "@angular/platform-browser": "16.2.12",
    "@angular/platform-browser-dynamic": "16.2.12",
    "@angular/router": "16.2.12",
    "@natu/ui-angular": "workspace:*",
    "@natu/utils": "workspace:*",
    "rxjs": "7.8.1",
    "tslib": "2.3.0",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.9",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular/cli": "16.2.9",
    "@angular/compiler-cli": "16.2.12",
    "@compodoc/compodoc": "1.1.22",
    "@mswjs/data": "0.15.0",
    "@natu/eslint-config-angular": "workspace:*",
    "@natu/tsconfig": "workspace:*",
    "@natu/vrt": "workspace:*",
    "@playwright/test": "1.39.0",
    "@storybook/addon-a11y": "7.5.2",
    "@storybook/addon-docs": "7.5.2",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/angular": "7.5.2",
    "@storybook/blocks": "7.5.2",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/angular": "14.3.0",
    "@testing-library/jasmine-dom": "1.3.3",
    "@testing-library/user-event": "14.5.1",
    "@types/jasmine": "4.3.5",
    "@types/node": "20.4.5",
    "@types/testing-library__jasmine-dom": "1.3.2",
    "cross-env": "7.0.3",
    "eslint": "8.52.0",
    "http-server": "14.1.1",
    "jasmine-axe": "1.1.0",
    "jasmine-core": "4.6.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "msw": "2.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "source-map-explorer": "2.5.3",
    "storybook": "7.5.2",
    "typescript": "5.1.6"
  },
  "msw": {
    "workerDirectory": "src"
  }
}
